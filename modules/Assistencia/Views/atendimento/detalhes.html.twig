{% extends 'baseLogged.html.twig' %}

{% block breadcrumb %}
    {% include 'breadcrumb.html.twig' with {'modulos': data.modulo} %}
{% endblock %}

{% block stylesheets %}
	{{parent()}}
	<style type="text/css">
		.disabled-select {
		   background-color:#d5d5d5;
		   opacity:0.5;
		   border-radius:3px;
		   cursor:not-allowed;
		   position:absolute;
		   top:0;
		   bottom:0;
		   right:0;
		   left:0;
		}
	</style>
{% endblock %}

{% block principal %}

    {% if flash.error is defined %}
        <div class="alert alert-danger alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            {{flash.error|raw}}
        </div>
    {% endif %}

    {% if flash.success is defined %}
        <div class="alert alert-success alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            {{flash.success|raw}}
        </div>
    {% endif %}


<form action="{% if data.permissao.permissao > 1 %} /{{data.modulo.url}}/detalhes {% else %} javascript:void(0); {% endif %}" method="post" role="form" class="form-atendimento-detalhes" enctype="multipart/form-data">

       
    <div class="card card-outline-info">
        <div class="card-header">
            <h4 class="m-b-0 text-white">Detalhes</h4>
        </div>
        <div class="card-body">

            <input type="hidden" name="voltar" value="{{data.voltar}}">

            {% if data.registro.id is defined %}
                <input type="hidden" name="id" id="campo-id" value="{{data.registro.id}}">
                <input type="hidden" name="protocolo" id="campo-id" value="{{data.registro.protocolo}}">

                <input type="hidden" name="_METHOD" value="PUT"/>
            {% endif %}

            <div class="row">


                <div class="col-md-6 col-lg-3 col-xlg-3">
                    <a href="/{{data.modulo.url}}/{{data.registro.id}}" target="_blank">
                        <div class="card card-inverse card-info">
                            <div class="box bg-info text-center">
                                <h5 class="font-light text-white"><strong>Protocolo</strong></h5>
                                <h5 class="text-white">{{data.registro.protocolo}}</h5>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 col-lg-3 col-xlg-3">
                    <div class="card card-dark card-inverse">
                        <div class="box text-center">
                            <h5 class="font-light text-white"><strong>Responsável</strong></h5>
                            <h5 class="text-white">{{data.registro.responsavel}}</h5>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3 col-xlg-3">
                    <div class="card card-inverse card-warning">
                        <div class="box text-center">
                            <h5 class="font-light text-white"><strong>Data de Abertura</strong></h5>
                            <h5 class="text-white">{{data.registro.criado_em|date("d/m/Y h:i:s")}}</h5>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3 col-xlg-3">
                    <div class="card card-inverse card-primary">
                        <div class="box text-center">
                            <h5 class="font-light text-white"><strong>Situação</strong></h5>
                            <h5 class="text-white">{{data.registro.descricao_status}}</h5>
                        </div>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Cliente</label>
                        <input type="text" name="item_descritivo" class="form-control" value="{{data.registro.cliente}}" readonly="readonly">
                    </div>
                </div>


                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Item</label>
                        <input type="text" name="item_descritivo" class="form-control" value="{{data.registro.cod_item}} - {{data.registro.desc_tecnica}}" readonly="readonly">
                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="form-group" id="grupo-serie">
                        <label>Número de Série</label>
                        <input type="text" name="numero_serie" readonly="readonly" id="campo-numero-serie"  class="form-control" placeholder="Informe o Número de Série" value="{% if data.registro.serie is defined %}{{data.registro.serie}}{% endif %}">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group" id="grupo-sequencial">
                        <label>Sequencial</label>
                        <input type="text"   readonly="readonly"  name="sequencial" maxlength="100" class="form-control" id="campo-sequencial"  placeholder="Informe o Sequencial" value="{% if data.registro.sequencial is defined %}{{data.registro.sequencial}}{% endif %}">
                    </div>
                </div>

                 <div class="col-lg-6">
                    <div class="form-group">
                        <label>Data Fabricação</label>
                        <input type="text" name="dt_fabricacao" readonly="readonly"  id="campo-data-fab" class="form-control data" placeholder="Informe a Data de Fabricação" value="{% if data.registro.dt_fabricacao is defined %}{{data.registro.dt_fabricacao}}{% endif %}">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Data da Compra</label>
                        <input type="text" name="dt_compra" readonly="readonly" class="form-control data" placeholder="Informe a Data de Compra" value="{% if data.registro.dt_compra is defined %}{{data.registro.dt_compra}}{% endif %}">
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Defeito Reclamado</label>
                        <input type="text" name="item_descritivo" class="form-control" value="{{data.registro.cod_defeito}} - {{data.registro.descricao_defeito}}" readonly="readonly">
                    </div>
                </div>


                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Relato do Cliente*</label>
                        <textarea name="obs_cliente" disabled="disabled" required="required" maxlength="3999" class="form-control" rows="6" style="resize:none;">{{data.registro.obs_cliente}}</textarea>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Retratação*</label>
                        <textarea name="obs_interna" disabled="disabled" maxlength="3999" required="required" class="form-control" rows="6" style="resize:none;">{{data.registro.obs_interna}}</textarea>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Cliente que enviou</label>
                        <input type="text" name="cliente_que_enviou" class="form-control" value="{{data.registro.descricao_cli_env}}" readonly="readonly">
                    </div>
                </div>


                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Nota Fiscal</label>
                        <input type="text" name="nota_fiscal" class="form-control" value="{{data.registro.nota_fiscal}}" readonly="readonly">
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Responsabilidade</label>
                        <select name="responsabilidade" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %} class="form-control">
                            <option></option>
                            <option value="R" {% if data.registro.responsabilidade is defined and data.registro.responsabilidade == 'R'%} selected="selected" {% endif %}>Resfri Ar</option>
                            <option value="C" {% if data.registro.responsabilidade is defined and data.registro.responsabilidade == 'C'%} selected="selected" {% endif %}>Cliente</option>
                        </select>
                    </div>
                </div>
                
                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Defeito Técnico</label>
                        {% if data.registro.bloqueia_campos == '1' %}
                            <input type="hidden" name="defeito_tecnico_id" value="{{data.registro.defeito_tecnico_id}}">
                        {% endif %}
                        <select name="defeito_tecnico_id" id="select-defeito" {% if data.registro.bloqueia_campos == '1' %} disabled="disabled" {% endif %}  class="form-control">
                            {% if data.registro.defeito_tecnico_id is defined %}
                                <option value="{{data.registro.defeito_tecnico_id}}">
                                    {{data.registro.cod_defeito_tecnico}} - {{data.registro.desc_defeito_tecnico}}
                                </option>
                            {% endif %}
                        </select>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Fornecedor Envolvido no Defeito</label>
                         {% if data.registro.bloqueia_campos == '1' %}
                            <input type="hidden" name="forn_defeito_id" value="{{data.registro.forn_defeito_id}}">
                        {% endif %}
                        <select name="forn_defeito_id" id="select-fornecedores" {% if data.registro.bloqueia_campos == '1' %} disabled="disabled" {% endif %}   class="form-control">
                            {% if data.registro.forn_defeito_id is defined %}
                                <option value="{{data.registro.forn_defeito_id}}">
                                    {{data.registro.cod_forn}} - {{data.registro.desc_forn}}
                                </option>
                            {% endif %}
                        </select>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Classificação do Defeito</label>
                        <select name="clas_defeito" class="form-control"  {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %}>
                            <option></option>
                            <option value="N" {% if data.registro.clas_defeito is defined and data.registro.clas_defeito == 'N'%} selected="selected" {% endif %} >Nenhum</option>
                            <option value="F" {% if data.registro.clas_defeito is defined and data.registro.clas_defeito == 'F'%} selected="selected" {% endif %}>Funcional</option>
                            <option value="V" {% if data.registro.clas_defeito is defined and data.registro.clas_defeito == 'V'%} selected="selected" {% endif %}>Visual</option>
                        </select>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Saída do Produto</label>
                        <select name="saida_produto" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %} class="form-control">
                            <option></option>
                            <option value="EV" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'EV'%} selected="selected" {% endif %}>Enviar nova e vender na Cidade</option>
                            <option value="AC" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'AC'%} selected="selected" {% endif %}>Acerto Comercial, Vender na Cidade</option>
                            <option value="EN" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'EN'%} selected="selected" {% endif %}>Enviar Nova e Sucatear</option>
                            <option value="JF" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'JF'%} selected="selected" {% endif %}>Já foi, Sucatear</option>
                            <option value="AS" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'AS'%} selected="selected" {% endif %}>Acerto Comercial, Sucatear</option>
                            <option value="MN" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'MN'%} selected="selected" {% endif %}>Manutenção enviar a Mesma</option>
                            <option value="OA" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'OA'%} selected="selected" {% endif %}>Orçamento Aprovado</option>
                            <option value="OR" {% if data.registro.saida_produto is defined and data.registro.saida_produto == 'OR'%} selected="selected" {% endif %}>Orçamento Reprovado</option>
                        </select>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Observação</label>
                        <textarea name="obs_interna_tecnica" maxlength="3999" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %} class="form-control" rows="6" style="resize:none;">{{data.registro.obs_interna_tecnica}}</textarea>
                    </div>
                </div>

                <div class="col-lg-12">
                    <h4><strong>Atendimentos Técnicos</strong></h4>
                    <div class="table-responsive">
                    <table class="table table-hover table-striped">
                        <thead>
                            <tr>
                                <th width="20%">Data</th>
                                <th width="30%">Colaborador</th>
                                <th width="40%">Descrição</th>
                                <th width="10%">
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for row in data.registro.atendimentos %}
                                <tr>
                                    <td>{{row.dt_atend|date('d/m/Y')}}</td>
                                    <td>{{row.responsavel}}</td>
                                    <td>{{row.atendimento|raw}}</td>
                                    <td align="right">
                                        <a href="javascript:void(0);"  class="btn-edit-atendimento" data-id="{{row.id}}" data-registro-id="{{row.registro_id}}"  data-dt-atendimento="{{row.dt_atend|date('d/m/Y')}}" data-atendimento="{{row.atendimento}}" data-laudo="{{row.considerar_laudo}}" data-protocolo="{{data.registro.protocolo}}" data-toggle="tooltip" data-original-title="Editar"><i class="fa fa-pencil text-inverse m-l-10"></i></a> 
                                        <a href="javascript:void(0);" class="btn-del-atendimento {% if row.permite_excluir == false %} disabled {% endif %}" data-id="{{row.id}}" data-protocolo="{{data.registro.protocolo}}" data-toggle="tooltip" data-original-title="Excluir"><i class="fa fa-close text-danger m-r-10"></i></a>
                                    </td>
                                </tr>
                            {% else %}
                                <tr><td colspan="4" align="center">Nenhum atendimento.</td></tr>
                            {% endfor %}
                            <tr>
                                <td colspan="3"></td>
                                <td align="right">
                                    <a href="javascript:void(0);" data-id="{{data.registro.id}}" {% if data.registro.bloqueia_campos == '1' %} style="display:none;" {% endif %}  data-protocolo="{{data.registro.protocolo}}" class="btn btn-success btn-xs btn-add-atendimento" title="Adicionar Atendimento"><i class="fa fa-plus"></i> Adicionar</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label class="control-label">Anexos</label>
                        <div class="form-group">
                            <input id="upload" type="file" name="files[]" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %} multiple>
                        </div>

                        {% if data.arquivos is defined and data.arquivos != null %}   
                            
                            <div class="row">
                                <div class="col-lg-12">
                                    <h3><strong>Arquivos</strong></h3>
                                    <div class="table-responsive">
                                    <table class="table table-hover table-striped">
                                        <thead>
                                            <tr>
                                                <th width="40%">Arquivo</th>
                                                <th width="40%">Data</th>
                                                <th width="20%"></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for file in data.arquivos %}
                                            <tr>
                                                <td>{{file.arquivo}}</td>
                                                <td>{{file.criado_em|date("d/m/Y H:m:i")}}</td>
                                                <td align="right">
                                                    <a href="/assistencia-tecnica/atendimento/arquivos/download/{{file.link}}" target="_blank" class="btn btn-default btn-xs" title="Download"><i class="fa fa-download"></i></a>
                                                    <a href="javascript:void(0);" class="btn-delete-file-modal" data-id="{{file.id}}" data-name="{{file.arquivo}}" title="Excluir" data-toggle="tooltip" data-original-title="Excluir"><i class="fa fa-close text-danger m-r-10"></i></a>
                                                </td>
                                            </tr>
                                        {% else %}
                                            <tr><td colspan="5" align="center">Nenhum arquivo.</td></tr>
                                        {% endfor %}
                                        </tbody>
                                    </table>
                                    </div>
                                </div>
                            </div>
                        {% endif %}
                    </div>
                </div>


                 <div class="col-lg-12">
                    <div class="form-group">
                        {% if data.arquivos_laudo  is not defined or   data.arquivos_laudo == null %}   
                            <label class="control-label">Imagem Laudo</label>
                            <div class="form-group">
                                <input id="upload-laudo" type="file" name="files_laudo" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %}>
                            </div>
                        {% endif %}

                        {% if data.arquivos_laudo is defined and data.arquivos_laudo != null %}   
                            
                            <div class="row">
                                <div class="col-lg-12">
                                    <h3><strong>Imagem Laudo</strong></h3>
                                    <div class="table-responsive">
                                    <table class="table table-hover table-striped">
                                        <thead>
                                            <tr>
                                                <th width="40%">Imagem</th>
                                                <th width="40%">Data</th>
                                                <th width="20%"></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for file in data.arquivos_laudo %}
                                            <tr>
                                                <td>{{file.arquivo}}</td>
                                                <td>{{file.criado_em|date("d/m/Y H:m:i")}}</td>
                                                <td align="right">
                                                    <a href="/assistencia-tecnica/atendimento/arquivos/laudo/download/{{file.link}}" target="_blank" class="btn btn-default btn-xs" title="Download"><i class="fa fa-download"></i></a>
                                                    <a href="javascript:void(0);" class="btn-delete-file-modal" data-id="{{file.id}}" data-name="{{file.arquivo}}" title="Excluir" data-toggle="tooltip" data-original-title="Excluir"><i class="fa fa-close text-danger m-r-10"></i></a>
                                                </td>
                                            </tr>
                                        {% else %}
                                            <tr><td colspan="5" align="center">Nenhum arquivo.</td></tr>
                                        {% endfor %}
                                        </tbody>
                                    </table>
                                    </div>
                                </div>
                            </div>
                        {% endif %}
                    </div>
                </div>

                 <div class="col-lg-12">
                    <div class="form-group">
                        <label>Responsável*</label>
                        <select name="responsavel_id" class="form-control" {% if data.registro.bloqueia_campos == '1' %} readonly="readonly" {% endif %} required="required">
                            <option></option>
                            {% for u in data.usuarios %}
                                <option value="{{u.id}}"  {% if data.registro.responsavel_id != null and u.id == data.registro.responsavel_id %} selected="selected" {% elseif data.registro.responsavel_id == null and usuario.id == u.id %} selected="selected"  {% endif %}>{{u.nome}}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Chamado ERP</label>
                        {% if data.registro.bloqueia_campos == '1' %}
                            <input type="hidden" name="chamado_erp_id" value="{{data.registro.chamado_erp_id}}">
                        {% endif %}
                        <select name="chamado_erp_id" {% if data.registro.bloqueia_campos == '1' %} disabled="disabled" {% endif %} id="select-chamados" class="form-control">
                           {% if data.registro.chamado_erp_id is defined %}
                                <option value="{{data.registro.chamado_erp_id}}">
                                    {{data.registro.num_chamado}}
                                </option>
                            {% endif %}
                        </select>
                    </div>
                </div>


                <div class="col-lg-12">
                    <div class="form-group">
                        <label>Status*</label>
                        <select name="status_id" class="form-control" required="required">
                            <option></option>
                            {% for status in data.status %}
                                <option value="{{status.id}}" {% if data.registro.status_id != null and status.id == data.registro.status_id %} selected="selected"  {% elseif data.registro.status_id == null and status.id == data.status_novo_chamado %} selected="selected" {% endif %}>{{status.descricao}}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="card card-body" style="margin-top: -20px;">
        <div class="row">
            <div class="col-lg-12">
                {% if data.permissao.permissao > 1 %}
                    <button type="submit" class="btn btn-success btn-submit" data-loading-text="Aguarde...">
                        <i class="fa fa-check"></i> Salvar
                    </button>
                {% endif %}
                <a href="{{data.voltar}}" class="btn btn-inverse waves-effect waves-light">
                    <i class="fa fa-times"></i> Cancelar
                </a>
                <a href="/{{data.modulo.url}}/detalhes/laudo/{{data.registro.id}}" target="_blank" class="btn btn-primary waves-effect waves-light">
                    <i class="fa fa-print"></i> Imprimir Laudo
                </a>
            </div>
        </div>
    </div>


</form>


 <div class="modal fade modal-delete-arquivo" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                Confirma a exclusão do Arquivo <strong></strong>?
            </div>
            <div class="modal-footer">
                <form action="/{{data.modulo.url}}/excluir/arquivo/laudo" method="post">
                    <input type="hidden" name="_METHOD" value="DELETE"/>
                    <input type="hidden" name="id" value="">
                    <input type="hidden" name="registro_id" value="{{data.registro.id}}">
                    <button type="button" class="btn btn-default btn-cancel" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary btn-submit-modal btn-success" data-loading-text="Excluindo...">Confirmar</button>
                </form>
            </div>
        </div>
    </div>
</div>

{# MODAL DE INSERÇÃO DE ATENDIMENTO NO CHAMADO #}
<div class="modal fade modal-adicionar-atendimento" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form action="javascript:void(0);" method="post" class="form-inline form-add-atendimento">
                <input type="hidden" name="registro_id" value="">
                <input type="hidden" name="id" value="">
                <div class="modal-body">
                    <h4><strong><strong class="titulo">Adicionar</strong> Atendimento - Protocolo #<strong class="protocolo"></strong></strong></h4>
                    <div class="form-group">
                        <div class="form-group m-lg-2">
                            <input type="text" name="dt_atend" class="form-control data" value="{{"now"|date("d/m/Y")}}" placeholder="Data" data-myrules="required" style="width:120px;">
                        </div>
                    </div>
                    <div class="form-group m-lg-2">
                        <textarea class="form-control" name="atendimento" style="width: 100%; resize: none;" maxlength="3000" rows="5" placeholder="Descrição" data-myrules="required"></textarea>
                    </div>
                    <div class="form-group m-lg-2">
                        <label>Considerar Atendimento no Laudo*</label>
                        <select name="considerar_laudo" style="width: 100%" required="required" class="form-control">
                            <option value="S">Sim</option>
                            <option value="N">Não</option>
                        </select>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 m-top-10">
                            <span class="notification fcolor-b13125"></span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="col-lg-12">
                        <span class="success m-right-20 fcolor-398439"></span>

                        <button type="button" class="btn btn-primary btn-success btn-adicionar-atendimento-submit" style="margin-top:10px;" data-loading-text="Adicionando...">Confirmar</button>
