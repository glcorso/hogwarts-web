{
    "name":"lidere/portal-resfriar",
    "version":"2.1",
    "description":"Portal Default - By Lidere Sistemas",
    "authors":[
        {
            "name":"Lidere Sistemas",
            "email":"suporte@lideresistemas.com.br"
        }
    ],
    "require":{
        "php": ">=5.5.9",
        "slim/slim":"2.*",
        "slim/views":"0.1.*",
        "twig/twig":"1.*",
        "phpmailer/phpmailer": "~6.0",
        "zeuxisoo/slim-whoops": "0.3.*",
        "lidere/slim-services": "1.0.1",
        "vlucas/phpdotenv": "2.2.0",
        "symfony/console": "2.8.2",
        "symfony/process": "2.8.2",
        "symfony/dependency-injection": "2.8.2",
        "illuminate/database": "5.2.*",
        "illuminate/events": "5.2.*",
        "illuminate/container": "5.2.*",
        "illuminate/translation": "5.2.*",
        "illuminate/filesystem": "5.2.*",
        "illuminate/validation": "5.2.*",
        "illuminate/pagination": "5.2",
        "mtdowling/cron-expression": "1.0.4",
        "justinrainbow/json-schema": "^5.2",
        "yajra/laravel-oci8": "4.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/lideresis/slim-services.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "Lidere\\" : "src/",
            "Lidere\\Modules\\": "modules/",
            "Slim\\": "src/Slim/",
            "Illuminate\\Pagination\\": "src/Illuminate/Pagination/",
            "Illuminate\\Validation\\": "src/Illuminate/Validation/"
       }
    }
}
